{
	"name": "BlobDs",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ConnBlobEmpFiles",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "@pipeline().parameters.SourceFile"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"container": "csv-files"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": ""
		},
		"schema": [
			{
				"name": "EmpId",
				"type": "String"
			},
			{
				"name": "EmpName",
				"type": "String"
			},
			{
				"name": "Salary",
				"type": "String"
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}